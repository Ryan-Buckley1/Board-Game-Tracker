<form class="signup-form">
  <div>
    <label for="email-signup">Email:</label>
    <input type="text" name="email-signup" id="email-signup" />
  </div>
  <div>
    <label for="username-signup">Username:</label>
    <input type="text" name="username-signup" id="username-signup" />
  </div>
  <div>
    <label for="first-name-signup">First Name:</label>
    <input type="text" name="first-name-signup" id="first-name-signup" />
  </div>
  <div>
    <label for="last-name-signup">Last Name:</label>
    <input type="text" name="last-name-signup" id="last-name-signup" />
  </div>
  <div>
    <label for="password-signup">Password:</label>
    <input type="text" name="password-signup" id="password-signup" />
  </div>
  <div>
    <button type="submit">Sign Up</button>
  </div>
</form>
<script src="/javascript/signup.js"></script>