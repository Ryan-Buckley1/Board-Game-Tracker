<link rel="stylesheet" href="/stylesheets/signUp.css">
<form class="custom-form">
  <div>
    <h1>
      Sign Up
    </h1>
  </div>
  <div>
    <label for="email-signup">Email:</label><br/>
    <input type="email" name="email-signup" id="email-signup" />
  </div>
  <div>
    <label for="username-signup">Username:</label><br/>
    <input type="text" name="username-signup" id="username-signup" />
  </div>
  <div>
    <label for="first-name-signup">First Name:</label><br/>
    <input type="text" name="first-name-signup" id="first-name-signup" />
  </div>
  <div>
    <label for="last-name-signup">Last Name:</label><br/>
    <input type="text" name="last-name-signup" id="last-name-signup" />
  </div>
  <div>
    <label for="password-signup">Password:</label><br/>
    <input type="password" name="password-signup" id="password-signup" />
  </div>
  <div>
    <button type="submit" class="btn btn-primary btn-signup">Sign Up</button>
  </div>
</form>
<script src="/javascript/signup.js"></script>